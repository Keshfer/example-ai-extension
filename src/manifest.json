{
	"manifest_version": 3,
	"name": "Banana Joker",
	"version": "0.1.0",
	"description": "LLM tells you a joke",
	"action": {
		"default_popup": "popup.html"

	},
	"permissions": [
		"storage"
	],
	"content_scripts": [
		{
		"matches": ["<all_urls>"],
		"js": ["content.js"]
		}
  	],
	"background": {
		"service_worker": "background.js"
	}
}